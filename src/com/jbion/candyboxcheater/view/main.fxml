<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="240.0" minWidth="320.0" prefHeight="600.0" prefWidth="800.0" stylesheets="@../application.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jbion.candyboxcheater.controllers.MainController">
   <center>
      <TabPane tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
            <Tab text="Info">
              <content>
                <AnchorPane minHeight="-Infinity" minWidth="-Infinity">
                     <children>
                        <ScrollPane fitToHeight="true" fitToWidth="true" layoutX="-1.0" layoutY="7.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                           <content>
                              <VBox maxWidth="1.7976931348623157E308" minHeight="-Infinity" spacing="15.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
                                 <children>
                                    <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" text="Welcome, Candy Cheater!" textAlignment="CENTER">
                                       <font>
                                          <Font name="System Bold" size="31.0" />
                                       </font>
                                    </Label>
                                    <Label maxWidth="1.7976931348623157E308" minHeight="-Infinity" text="This program is a variable editor for the game Candybox 2. This game can be played at candybox2.net, where you'll normally get 1 candy per second." wrapText="true">
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </Label>
                                    <VBox minHeight="-Infinity" spacing="10.0">
                                       <children>
                                          <Label maxWidth="1.7976931348623157E308" text="Get ready to cheat:">
                                             <font>
                                                <Font name="System Bold" size="14.0" />
                                             </font>
                                          </Label>
                                          <Label lineSpacing="5.0" maxWidth="1.7976931348623157E308" minHeight="-Infinity" text="1. Wait for 30 current candies to get the developer request button&#10;2. Add all options by clicking that new button&#10;3. Go to the &quot;save&quot; tab, &quot;Saving&quot; section, and click on &quot;Get the current game as text&quot;&#10;4. Copy the text and paste it below, and click the &quot;Parse Save&quot; button&#10;5. Visit other tabs of this program and enjoy cheating!" wrapText="true">
                                             <VBox.margin>
                                                <Insets />
                                             </VBox.margin>
                                             <padding>
                                                <Insets left="20.0" />
                                             </padding>
                                          </Label>
                                       </children>
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </VBox>
                                    <VBox minHeight="-Infinity" spacing="10.0">
                                       <children>
                                          <Label maxWidth="1.7976931348623157E308" text="When you're done changing stuff:">
                                             <font>
                                                <Font name="System Bold" size="14.0" />
                                             </font>
                                          </Label>
                                          <Label lineSpacing="5.0" maxWidth="1.7976931348623157E308" minHeight="-Infinity" text="1. Click the &quot;Copy to clipboard&quot; button&#10;2. Go back to the website and go to the &quot;Save&quot; tab&#10;3. Paste the text in the &quot;Loading&quot; section&#10;4. Click &quot;Load&quot; and enjoy the new values!" wrapText="true">
                                             <padding>
                                                <Insets left="20.0" />
                                             </padding>
                                          </Label>
                                       </children>
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </VBox>
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </padding>
                              </VBox>
                           </content>
                        </ScrollPane>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
            <Tab text="Stocks">
              <content>
                <AnchorPane minHeight="-Infinity" minWidth="-Infinity">
                     <children>
                        <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                           <content>
                              <fx:include source="stocks.fxml" />
                           </content>
                        </ScrollPane>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
            <Tab text="Progress">
               <content>
                  <AnchorPane minHeight="-Infinity" minWidth="-Infinity">
                     <children>
                        <ScrollPane minHeight="-Infinity" minWidth="-Infinity" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                           <content>
                              <fx:include source="progress.fxml" />
                           </content></ScrollPane>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Equipment">
              <content>
                <AnchorPane minHeight="-Infinity" minWidth="-Infinity">
                     <children>
                        <ScrollPane layoutX="220.0" layoutY="75.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                           <content>
                              <fx:include source="equipment.fxml" />
                           </content></ScrollPane>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
            <Tab text="Inventory">
              <content>
                <AnchorPane minHeight="-Infinity" minWidth="-Infinity">
                     <children>
                        <ScrollPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                           <content>
                              <fx:include source="inventory.fxml" />
                           </content></ScrollPane>
                     </children></AnchorPane>
              </content>
            </Tab>
            <Tab text="Miscellaneous">
              <content>
                <AnchorPane minHeight="-Infinity" minWidth="-Infinity">
                     <children>
                        <ScrollPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                           <content>
                              <fx:include source="miscellaneous.fxml" />
                           </content></ScrollPane>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </TabPane>
   </center>
   <bottom>
      <BorderPane BorderPane.alignment="CENTER">
         <center>
            <TextArea fx:id="rawText" maxHeight="-Infinity" onInputMethodTextChanged="#parseSaveText" prefHeight="50.0" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <Label maxWidth="1.7976931348623157E308" text="Candybox save content:" BorderPane.alignment="CENTER" />
         </top>
         <right>
            <VBox spacing="5.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="reparseBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#parseSaveText" text="Parse save" />
                  <Button fx:id="clipboardBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#copyToClipboard" text="Copy to clipboard" />
               </children>
               <BorderPane.margin>
                  <Insets bottom="5.0" left="5.0" top="5.0" />
               </BorderPane.margin>
            </VBox>
         </right>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </BorderPane>
   </bottom>
</BorderPane>
